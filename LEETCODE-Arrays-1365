class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int n=nums.length;        
        int [] ans=new int[n];
        for(int i=0;i<n;i++)
        {
            int count=0;
            int j=0;
            while(j<n)
            {
                if(nums[i]>nums[j]) count++;
                j++;
            }
            ans[i]=count;
        }
        return ans;
    }
}
